    "def discrete_log(a: int, g: int, p: int) -> int:\n",
    "  mover = g\n",
    "  i=1\n",
    "  while i < p:\n",
    "    if mover == a:\n",
    "      return i\n",
    "    mover *= g\n",
    "    mover %= p\n",
    "    i += 1\n",
    "  return 0\n",
    "discrete_log(6, 5, 7)"
   ]
  }
